{
  "name": "bb-nufflizier",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:nufflizier": "vitest run tests/unit/nufflizier*.test.ts && playwright test tests/e2e/smoke.spec.ts",
    "test:legacy": "vitest run tests/unit/analyzeReplay.test.ts tests/unit/coachingOutput.test.ts tests/unit/teamScoping.test.ts tests/unit/demoReplayStructured.test.ts tests/unit/rulesPlayerLinking.test.ts",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "nufflizier": "node --import tsx src/cli/nufflizier.ts"
  },
  "bin": {
    "nufflizier": "./bin/nufflizier"
  },
  "dependencies": {
    "fast-xml-parser": "^5.1.0",
    "next": "^14.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "tsx": "^4.19.2",
    "vitest": "^2.1.5"
  }
}
